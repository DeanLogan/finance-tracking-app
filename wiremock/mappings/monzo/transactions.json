{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPath": "/transactions",
        "queryParameters": {
          "current_account_id": {
            "matches": ".*"
          }
        }
      },
      "response": {
        "status": 200,
        "bodyFileName": "monzo/transactions/{{request.query.account_id}}.json",
        "transformers": ["response-template"],
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ]
}